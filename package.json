    "buba": "^2.0.3",
    "eslint": "^3.4.0",
    "eslint-import-resolver-babel-module": "^2.0.1",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-immutable": "^1.0.0",
